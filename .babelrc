{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline",
      "plugins": []
    },
    "production": {
      "sourceMaps": false,
      "plugins": [],
      "ignore": [
        "**/__tests__/**"
      ]
    }
  }
}